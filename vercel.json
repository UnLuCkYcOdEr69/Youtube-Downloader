{
  "builds": [
    { "src": "api/*.py", "use": "@vercel/python" },
    { "src": "templates/**/*.html", "use": "@vercel/static" },
    { "src": "static/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/download.py" },
    { "src": "/(.*)", "dest": "/templates/index.html" }
  ]
}
